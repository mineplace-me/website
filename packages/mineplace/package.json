{
  "name": "mineplace",
  "description": "Main Mineplace website",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/bun/vite.config.mts",
    "build.types": "tsc --incremental --noEmit",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=en-US,es-ES,ko-KR,nl-NL,pt-PT,ru-RU,tr-TR,zh-CN --assetsPath=i18n",
    "serve": "bun server/entry.bun.js",
    "start": "vite --open --mode ssr --host",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@anuragroy/tailwindcss-animate": "^1.0.6",
    "@builder.io/qwik": "^1.16.0",
    "@builder.io/qwik-city": "^1.16.0",
    "@eslint/js": "latest",
    "@luminescent/ui": "^5.2.0",
    "@luminescent/ui-qwik": "^5.2.0",
    "@tailwindcss/vite": "^4.1.12",
    "@types/bun": "*",
    "@types/node": "24.3.0",
    "eslint": "9.34.0",
    "eslint-plugin-qwik": "^1.16.0",
    "globals": "16.3.0",
    "lucide-icons-qwik": "^0.540.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.12",
    "typescript": "5.8.3",
    "typescript-eslint": "8.41.0",
    "typescript-plugin-css-modules": "latest",
    "undici": "*",
    "vite": "7.1.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
